<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ActionBarBuzzTheme" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">#57e2ca</item>
    </style>
    <style name="EditTextBuzzTheme" parent="android:Widget.EditText">
        <item name="android:background">@drawable/buzztheme_edit_text_holo_light</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="AutoCompleteTextViewBuzzTheme" parent="android:Widget.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/buzztheme_list_selector_holo_light</item>
        <item name="android:background">@drawable/buzztheme_edit_text_holo_light</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="CheckBoxBuzzTheme" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/buzztheme_btn_check_holo_light</item>
    </style>
    <style name="RadioButtonBuzzTheme" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/buzztheme_btn_radio_holo_light</item>
    </style>
    <style name="ButtonBuzzTheme" parent="android:Widget.Button">
        <item name="android:background">@drawable/buzztheme_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="ImageButtonBuzzTheme" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/buzztheme_btn_default_holo_light</item>
    </style>
    <style name="SpinnerBuzzTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/buzztheme_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/buzztheme_list_selector_holo_light</item>
    </style>
    <style name="SpinnerDropDownItemBuzzTheme" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/buzztheme_btn_radio_holo_light</item>
    </style>
    <style name="TabBuzzTheme">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/buzztheme_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>
    <style name="TabTextBuzzTheme">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>
    <style name="ProgressBarBuzzTheme" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/buzztheme_progress_horizontal_holo_light
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/buzztheme_progress_indeterminate_horizontal_holo_light
        </item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>
    <style name="SeekBarBuzzTheme" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">
            @drawable/buzztheme_scrubber_progress_horizontal_holo_light
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/buzztheme_scrubber_progress_horizontal_holo_light
        </item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
        <item name="android:thumb">@drawable/buzztheme_scrubber_control_selector_holo_light</item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
    </style>
    <style name="RatingBarBuzzTheme" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/buzztheme_ratingbar_full_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/buzztheme_ratingbar_full_holo_light
        </item>
    </style>
    <style name="RatingBarBigBuzzTheme" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/buzztheme_ratingbar_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/buzztheme_ratingbar_holo_light</item>
        <item name="android:minHeight">35dip</item>
        <item name="android:maxHeight">35dip</item>
    </style>
    <style name="RatingBarSmallBuzzTheme" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/buzztheme_ratingbar_small_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/buzztheme_ratingbar_small_holo_light
        </item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>
    <style name="ToggleBuzzTheme" parent="android:Widget.Button.Toggle">
        <item name="android:background">@drawable/buzztheme_btn_toggle_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="ListViewBuzzTheme" parent="android:Widget.ListView">
        <item name="android:listSelector">@drawable/buzztheme_list_selector_holo_light</item>
    </style>
    <style name="ListViewBuzzTheme.White" parent="android:Widget.ListView.White">
        <item name="android:listSelector">@drawable/buzztheme_list_selector_holo_light</item>
    </style>
    <style name="SpinnerItemBuzzTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textColor">#ffffff</item>
    </style>

</resources>